{
  "name": "badger",
  "version": "0.0.0",
  "private": true,
  "main": "index.js",
  "author": "websvcPT",
  "scripts": {
    "build": "rm -rf ./dist/*  && babel src -d dist --source-maps --minified ",
    "serve": "NODE_ENV=production node dist/index.js",
    "start": "NODE_ENV=development babel-node src/index.js",
    "dev": "DEBUG=server:debug API_KEY=test NODE_ENV=development nodemon src/index.js --exec babel-node",
    "test": "DEBUG=server:debug API_KEY=test NODE_ENV=test mocha --require babel-core/register --reporter spec --exit   tests/ --exec babel-node",
    "eslint": "eslint src/**/*.js --ignore-pattern \"node_modules/\" --ignore-pattern \"dist/\""
  },
  "babel": {
    "presets": [
      "node8"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "eslintConfig": {
    "plugins": [
      "node"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:node/recommended"
    ],
    "rules": {
      "node/no-unsupported-features/es-syntax": 0,
      "node/no-unsupported-features/es-builtins": 0
    },
    "env": {
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "sourceType": "module",
      "ecmaVersion": 2018
    }
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "gh-badges": "^2.2.1",
    "glob": "^7.1.6",
    "lower-case": "^1.1.4",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-node8": "^1.2.0",
    "chai": "^4.2.0",
    "debug": "^2.6.9",
    "eslint": "^6.6.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "install": "^0.13.0",
    "mocha": "^6.2.3",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
